<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/21" fx:controller="br.edu.ifsc.fln.controller.MarcaController">
    <TableView fx:id="tableViewMarca" layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="400.0">
        <columns>
            <TableColumn fx:id="tableColumnId" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="tableColumnNome" prefWidth="324.0" text="Nome" />
        </columns>
    </TableView>
    <Button layoutX="14.0" layoutY="214.0" mnemonicParsing="false" onAction="#loadData" text="Carregar Dados" />
</AnchorPane>
